	%$ Value = {Sine of time: (t/8, t/4, t/2, t).} $%
%}

{%
	%$ Name = {_CosTime} $%
	%$ Type = {float4} $%
	%$ Value = {Cosine of time: (t/8, t/4, t/2, t).} $%
%}

{%
	%$ Name = {unity_DeltaTime} $%
	%$ Type = {float4} $%
	%$ Value = {Delta time: (dt, 1/dt, smoothDt, 1/smoothDt).} $%
%}

{%
	%$ Name = {unity_LODFade} $%
	%$ Type = {float4} $%
	%$ Value = {Delta time: (dt, 1/dt, smoothDt, 1/smoothDt).} $%
%}

{%
	%$ Name = {_TextureSampleAdd} $%
	%$ Type = {float4} $%
	%$ Value = {Delta time: (dt, 1/dt, smoothDt, 1/smoothDt).} $%
%}

